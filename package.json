    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "espree": "^10.4.0",
    "husky": "^9.0.0",
    "jest": "^30.0.4",
